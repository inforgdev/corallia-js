{
  "name": "corallia-js",
  "version": "0.4.0",
  "description": "OctopoJS AST generator for design system tokens.",
  "main": "./dist/corallia.cjs",
  "module": "./dist/corallia.js",
  "exports": {
    ".": {
      "require": "./dist/corallia.cjs",
      "import": "./dist/corallia.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "main:build": "rollup --config rollup.config.js",
    "example:build": "node src/example/index.js"
  },
  "keywords": [
    "design-system",
    "tokens",
    "stringify",
    "js",
    "css",
    "scss",
    "less",
    "lesscss",
    "stylus"
  ],
  "author": "inforgdev",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "rollup": "^2.75.6",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "hex-rgb": "^5.0.0",
    "octopo-js": "github:inforgdev/octopo-js"
  }
}
